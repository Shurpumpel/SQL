        </Field>
        <Field Name="Company">
          <DataField>Company</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     COUNT(orders.OrderNo) AS [Количество заказов], employee.LastName, employee.FirstName, customer.Company
FROM         customer INNER JOIN
                      orders ON customer.CustNo = orders.CustNo INNER JOIN
                      employee ON orders.EmpNo = employee.EmpNo
GROUP BY customer.CustNo, orders.CustNo, orders.EmpNo, employee.EmpNo, employee.LastName, employee.FirstName, customer.Company</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
