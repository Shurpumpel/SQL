  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     Plane.Name, COUNT(Plane.Id_Plane) AS Expr2, Company.NameOfCompany
FROM         Plane INNER JOIN
                      Company ON Plane.Id_Company = Company.Id_Company
GROUP BY Plane.Id_Company, Company.Id_Company, Plane.Name, Company.NameOfCompany</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
