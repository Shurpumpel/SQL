  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     Company.NameOfCompany, COUNT(Plane.Id_Plane) AS [Количество самолетов]
FROM         Company INNER JOIN
                      Plane ON Company.Id_Company = Plane.Id_Company
GROUP BY Company.NameOfCompany</CommandText>
      </Query>
