  <DataSets>
    <DataSet Name="dsEmployee">
      <Query>
        <DataSourceName>DBdemoDS</DataSourceName>
        <CommandText>SELECT     employee.LastName, employee.FirstName, COUNT(orders.OrderNo) AS [Число заказов]
FROM         employee INNER JOIN
                      orders ON employee.EmpNo = orders.EmpNo
GROUP BY employee.EmpNo, employee.LastName, employee.FirstName
ORDER BY employee.LastName</CommandText>
      </Query>
